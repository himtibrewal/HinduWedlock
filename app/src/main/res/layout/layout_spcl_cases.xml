<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true">

    <LinearLayout xmlns:custom="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout style="@style/LinearLayout_Spinner" />

        <com.colaborotech.thehinduwedlock.utility.CustomLayoutTitleValue
            android:id="@+id/ctv_challenged_spcl_cses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            custom:heading="Challenged"
            custom:value="@string/not_filled" />

        <LinearLayout style="@style/LinearLayout_Spinner" />

        <com.colaborotech.thehinduwedlock.utility.CustomLayoutTitleValue
            android:id="@+id/ctv_thalassemia_spcl_cses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            custom:heading="Thalassemia"
            custom:value="@string/not_filled" />

        <LinearLayout style="@style/LinearLayout_Spinner" />

        <com.colaborotech.thehinduwedlock.utility.CustomLayoutTitleValue
            android:id="@+id/ctv_hiv_spcl_cses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            custom:heading="Hiv+?"
            custom:value="@string/not_filled" />

        <LinearLayout style="@style/LinearLayout_Spinner" />


    </LinearLayout>
</ScrollView>


            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_user_id_user_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/_3sdp"
                android:text="ZXRT3899"
                android:textSize="@dimen/_18sdp" />

            <TextView
                android:id="@+id/tv_lastonline_user_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/tv_user_id_user_detail"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:text="last online  today" />


            <TextView
                android:id="@+id/tv_servicetype_user_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:padding="@dimen/_5sdp"
                android:text="ZXRT3899"
                android:textColor="@color/red_dark"
                android:textSize="@dimen/_14sdp" />


        </RelativeLayout>

        <RelativeLayout

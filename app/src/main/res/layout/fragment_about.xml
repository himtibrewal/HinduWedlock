                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="B.Tech" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/fifth_row_search_result"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/fourth_row_search_result"
